// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model UserName {
id  String  @id @default(auto()) @map("_id") @db.ObjectId
username String?

}


model InvestmentUser {
  id         String    @id @default(auto()) @map("_id") @db.ObjectId
  username   String?
  password   String?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime? @updatedAt
  deleted_at DateTime? @map("deleted_at")
}

model MastertIncomeSystem {
  id                      String    @id @default(auto()) @map("_id") @db.ObjectId
  comSum                  Int?
  interestUFA             Int?
  interestBETFLIX         Int?
  comA                    Int?
  fullPayAgent            Int?
  actualPayAgent          Int?
  totalLoss               Int?
  percentage              Int?
  income                  Int?
  recommendedValueUFRUU   Int?
  recommendedValueUFRUV   Int?
  recommendedValueUFRUV02 Int?
  createdAt               DateTime? @default(now())
  updatedAt               DateTime? @updatedAt
  deleted_at              DateTime? @map("deleted_at")
}

model Member {
  id            String    @id @default(auto()) @map("_id") @db.ObjectId
  username      String?
  password      String?
  firstname     String?
  lastname      String?
  bankAccount   String?
  bank          String?
  phone         String?
  line          String?
  email         String?
  emailVerified Boolean?  @default(false)
  createdAt     DateTime? @default(now())
  updatedAt     DateTime? @updatedAt
  deleted_at    DateTime? @map("deleted_at")
  userAG        UserAG[]
}

}


model PastAG {
  id         String    @id @default(auto()) @map("_id") @db.ObjectId
  online     Int?
  comC       Int?
  loseC      Int?
  comM       Int?
  loseM      Int?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime? @updatedAt
  deleted_at DateTime? @map("deleted_at")
}

}


